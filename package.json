{
  "name": "find-waves-api",
  "version": "1.0.0",
  "description": "Typescript NodeJs API",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --transpile-only src/index.ts",
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "lint": "@eslint ./src ./test --ext .ts",
    "lint:fix": "@eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuanFreitasRibeiro/FindWavesApi.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Luan Freitas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuanFreitasRibeiro/FindWavesApi/issues"
  },
  "homepage": "https://github.com/LuanFreitasRibeiro/FindWavesApi#readme",
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "config": "^3.3.7",
    "express": "^4.17.3",
    "module-alias": "^2.2.2"
  }
